<% date = Time.parse("28-06-2013") %>
<% while date <= Time.parse("23-07-2013") %>
	<% User.all.each do |user| %>
		<% w = Worktime.where("user_id = ? and checkin > ? and checkin < ?", user.id, date, date.advance(:hours => 24)) %>
		<% if !w.blank? %>
			<%= date.strftime("%Y-%m-%d") %>&#09;<%= user.id.to_s %>&#09;true
			<br/>
		<% end %>
	<% end %>
	<% date += 1.day %>
<% end %>